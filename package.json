{
  "name": "node-base-template",
  "version": "0.0.1",
  "description": "Base template for starting a node server project.",
  "main": "server.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src",
    "test": "./node_modules/.bin/mocha -R spec test/",
    "start": "./node_modules/.bin/forever start --append -o ./forever.log -e ./forever.log lib/server.js",
    "stop": "./node_modules/.bin/forever stop lib/server.js",
    "build": "./node_modules/.bin/babel src --out-dir lib --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grumpyseal/node-base-template.git"
  },
  "keywords": [
    "Node.js",
    "npm"
  ],
  "author": "Kevin Ethridge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grumpyseal/node-base-template/issues"
  },
  "homepage": "https://github.com/grumpyseal/node-base-template#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.2.0",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "bunyan": "^1.7.0",
    "forever": "^0.15.1",
    "restify": "^4.0.4",
    "yaml-config": "^0.3.0"
  }
}
